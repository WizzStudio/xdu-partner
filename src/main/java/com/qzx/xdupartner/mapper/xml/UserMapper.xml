<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qzx.xdupartner.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qzx.xdupartner.entity.User">
        <id column="id" property="id"/>
        <result column="nick_name" property="nickName"/>
        <result column="major_name" property="majorName"/>
        <result column="stu_id" property="stuId"/>
        <result column="phone" property="phone"/>
        <result column="is_admin" property="isAdmin"/>
        <result column="qq" property="qq"/>
        <result column="icon" property="icon"/>
        <result column="picture" property="picture"/>
        <result column="my_description" property="myDescription"/>
        <result column="high_tag" property="highTag"/>
        <result column="mbti" property="mbti"/>
        <result column="constellation" property="constellation"/>
        <result column="creat_time" property="creatTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_deleted" property="isDeleted"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, nick_name, major_name, stu_id, phone, is_admin, qq, icon, picture, my_description, high_tag, mbti,
        constellation, creat_time, update_time, is_deleted
    </sql>

</mapper>
